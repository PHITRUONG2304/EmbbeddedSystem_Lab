#include <stdio.h>
#include <stdbool.h>
#include <unistd.h>
#include "freertos/FreeRTOS.h"


void vApplicationIdleHook( void )
{
/* This hook function does nothing but increment a counter . */
    ulIdleCycleCount1++;
}
void vApplicationTickHook( void )
{
/* This hook function does nothing but increment a counter . */
}


void app_main(void)
{
    while (true) {
        printf("Hello from app_main!\n");
        sleep(1);
    }
}
